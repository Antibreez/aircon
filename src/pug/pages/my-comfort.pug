							.input__label
								| E-mail
							.input__field
								input(type="email" name="email" data-validation={
									regex: ['^\\b[A-Z0-9._%+-]+@[A-Z0-9.-]+\\.[A-Z]{2,6}\\b$', 'i'],
									maxlength: 254,
									messages: {
										regex: "Неверный или некорректный e-mail адрес."
									}
								} required)
						.input
							.input__label
