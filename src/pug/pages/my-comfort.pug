							label
								input(type="radio" name="type" value="Мой комфорт премиум" checked required)
								i
								span Мой комфорт премиум
						.form__row
							.form__col
								.input
									.input__label
										| Имя
									.input__field
										input(type="text" name="firstname" data-validation={
										regex: ['^[^\\d]+$', 'ig'],
										maxlength: 300,
										messages: {
											regex: "Поле может содержать только буквы и символы"
										}
									})
							.form__col
								.input
									.input__label
										| Фамилия
									.input__field
										input(type="text" name="surname" data-validation={
										regex: ['^[^\\d]+$', 'ig'],
										maxlength: 300,
										messages: {
											regex: "Поле может содержать только буквы и символы"
										}
									})
